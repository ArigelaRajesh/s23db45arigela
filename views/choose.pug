extends layout

block content
  h1= title
  - var image_names=["airpods.jpeg","camera.jpeg","Lappy.jpeg","Mobile.jpeg","watch.jpeg"]
  - for(let r=0; r<3; r++){
     - let ranNum = Math.floor(Math.random()*5)
     img(src=`/images/${image_names[ranNum]}` alt=`${ranNum}` height="325px" width="325px") 
  - }